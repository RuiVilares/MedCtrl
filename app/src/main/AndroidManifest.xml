<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medctrl.mdteam.medctrl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AlarmListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.ManagementActivity"
            android:label="@string/title_activity_management"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.ConfigurationActivity"
            android:label="@string/title_activity_configuration"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.LoadingActivitie"
            android:label="@string/title_activity_loading_activitie"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.AddMedicationActivitie"
            android:label="@string/title_activity_add_medication_activitie"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.ProfileActivitie"
            android:label="@string/title_activity_profile_activitie"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>